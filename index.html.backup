                            if (filtered.length > 0) {
                                const catName = {
                                    hu: `DokumentÃ¡ciÃ³ÂƒÃ¡ciÃÂƒÃ³k - ${docSubcategories[subcat]?.hu || subcat}`,
                                    en: `Documentation - ${docSubcategories[subcat]?.en || subcat}`
                                };
                                renderCategory(`docs-${subcat}`, catName, filtered);
                                displayedFiles += filtered.length;
                            }
                            totalFiles += files.length;
                        }
                    }
                }
            }

            // Simulations fÃ¡jlÂƒÃ¡jlok
            if (category === 'all' || category === 'simulations') {
                if (fileData.simulations && Array.isArray(fileData.simulations)) {
                    const files = fileData.simulations.filter(f => {
                        const matchesSearch = matchesSearchFilter(f, searchTerm);
                        const matchesSecurity = matchesSecurityFilter(f, securityFilter);
                        return matchesSearch && matchesSecurity;
                    });
                    if (files.length > 0) {
                        renderCategory('simulations', categoryNames.simulations, files);
                        displayedFiles += files.length;
                    }
                    totalFiles += fileData.simulations.length;
                }
            }

            // Case studies fÃ¡jlÂƒÃ¡jlok
            if (category === 'all' || category === 'case_studies') {
                if (fileData.case_studies && Array.isArray(fileData.case_studies)) {
                    const files = fileData.case_studies.filter(f => {
                        const matchesSearch = matchesSearchFilter(f, searchTerm);
                        const matchesSecurity = matchesSecurityFilter(f, securityFilter);
                        return matchesSearch && matchesSecurity;
                    });
                    if (files.length > 0) {
                        renderCategory('case_studies', categoryNames.case_studies, files);
                        displayedFiles += files.length;
                    }
                    totalFiles += fileData.case_studies.length;
                }
            }

            // Assets fÃ¡jlÂƒÃ¡jlok
            if (category === 'all' || category === 'assets') {
                if (fileData.assets && Array.isArray(fileData.assets)) {
                    const files = fileData.assets.filter(f => {
                        const matchesSearch = matchesSearchFilter(f, searchTerm);
                        const matchesSecurity = matchesSecurityFilter(f, securityFilter);
                        return matchesSearch && matchesSecurity;
                    });
                    if (files.length > 0) {
                        renderCategory('assets', categoryNames.assets, files);
                        displayedFiles += files.length;
                    }
                    totalFiles += fileData.assets.length;
                }
            }

            if (displayedFiles === 0 && (searchTerm || securityFilter !== 'all')) {
                content.innerHTML = '<div class="no-results">Nincs talÃÂƒÃ¡lat / No results found</div>';
            }

            const totalFilesEl = document.getElementById('totalFiles');
            const displayedFilesEl = document.getElementById('displayedFiles');
            if (totalFilesEl) totalFilesEl.textContent = totalFiles;
            if (displayedFilesEl) displayedFilesEl.textContent = displayedFiles;
            
            // Debug log
            console.log('RenderFiles:', {
                category,
                searchTerm,
                securityFilter,
                totalFiles,
                displayedFiles,
                core: fileData.core?.length || 0,
                specs: fileData.specs?.length || 0,
                simulations: fileData.simulations?.length || 0,
                case_studies: fileData.case_studies?.length || 0,
                assets: fileData.assets?.length || 0,
                docs: fileData.docs ? Object.values(fileData.docs).reduce((sum, arr) => sum + (arr?.length || 0), 0) : 0
            });
        }

        function renderCategory(categoryId, categoryName, files) {
            const content = document.getElementById('content');
            
            const section = document.createElement('div');
            section.className = 'category-section';
            section.id = categoryId;
            
            const header = document.createElement('div');
            header.className = 'category-header';
            header.innerHTML = `
                <h2>${categoryName.hu} / ${categoryName.en}</h2>
                <span class="category-count">${files.length} fÃ¡jlÂƒÃ¡jl / files</span>
            `;
            
            const grid = document.createElement('div');
            grid.className = 'file-grid';
            
            files.forEach(file => {
                const card = document.createElement('div');
                const securityLevel = getSecurityLevel(file);
                card.className = `file-card security-${securityLevel}`;
                card.onclick = () => {
                    if (file.path) {
                        // FÃ¡jl Ãºtvonal helyes URL-encode-olÃ¡sa UTF-8 karakterekkel
                        // HasznÃ¡ljuk a /file/ endpoint-ot, ami kezeli az UTF-8 karaktereket
                        const pathParts = file.path.split('/');
                        const encodedPath = pathParts.map(part => encodeURIComponent(part)).join('/');
                        window.open('/file/' + encodedPath, '_blank');
                    }
                };
                
                const securityLabelHu = file.security_label_hu || '';
                const securityLabelEn = file.security_label_en || '';
                const securityWarningHu = file.security_warning_hu || '';
                const securityWarningEn = file.security_warning_en || '';
                
                const isCritical = isCriticalFile(file.name);
                const deleteButton = !isCritical ? `<button class="delete-btn" onclick="event.stopPropagation(); deleteFile('${escapeHtml(file.path)}', '${escapeHtml(file.name)}')">ğŸ—‘ï¸ TÃ¶rlÃ©s / Delete</button>` : '';
                
                card.innerHTML = `
                    <div class="file-header">
                        <div class="file-name">${escapeHtml(file.name)}</div>
                        <div style="display: flex; gap: 8px; align-items: center; flex-wrap: wrap;">
                            <span class="file-type">${file.type}</span>
                            ${securityLabelHu ? `<span class="security-badge security-${securityLevel}">${securityLabelHu} / ${securityLabelEn}</span>` : ''}
                        </div>
                    </div>
                    <div class="file-path">${escapeHtml(file.path)}</div>
                    ${securityWarningHu ? `
                    <div class="security-warning ${securityLevel === 'critical' ? 'critical' : ''}">
                        <div class="security-warning-title">${escapeHtml(securityWarningHu)}</div>
                        ${securityWarningEn ? `<div style="font-size: 0.9em; color: #666; margin-top: 5px;">${escapeHtml(securityWarningEn)}</div>` : ''}
                    </div>
                    ` : ''}
                    <div class="file-description">
                        <div class="description-lang">
                            <div class="lang-label">ğŸ‡­ğŸ‡º Magyar:</div>
                            <div class="description-text">${escapeHtml(file.description_hu)}</div>
                        </div>
                        <div class="description-lang">
                            <div class="lang-label">ğŸ‡¬ğŸ‡§ English:</div>
                            <div class="description-text">${escapeHtml(file.description_en)}</div>
                        </div>
                    </div>
                    <div class="file-summary">
                        <div class="file-summary-title">Tartalmi kivonat / Summary:</div>
                        <div class="file-summary-text">${escapeHtml(file.summary)}</div>
                    </div>
                    ${deleteButton}
                `;
                
                grid.appendChild(card);
            });
            
            section.appendChild(header);
            section.appendChild(grid);
            content.appendChild(section);
        }

        function matchesSearchFilter(file, searchTerm) {
            if (!searchTerm) return true;
            const term = searchTerm.toLowerCase();
            return (
                (file.name && file.name.toLowerCase().includes(term)) ||
                (file.path && file.path.toLowerCase().includes(term)) ||
                (file.description_hu && file.description_hu.toLowerCase().includes(term)) ||
                (file.description_en && file.description_en.toLowerCase().includes(term)) ||
                (file.summary && file.summary.toLowerCase().includes(term)) ||
                (file.type && file.type.toLowerCase().includes(term)) ||
                (file.security_label_hu && file.security_label_hu.toLowerCase().includes(term)) ||
                (file.security_label_en && file.security_label_en.toLowerCase().includes(term)) ||
                (file.content && file.content.toLowerCase().includes(term)) // FÃ¡jlÂƒÃ¡jl tartalom keresÃÂƒÃ©se
            );
        }

        function matchesSecurityFilter(file, securityFilter) {
            if (securityFilter === 'all') return true;
            const fileSecurityLevel = getSecurityLevel(file);
            return fileSecurityLevel === securityFilter;
        }

        function escapeHtml(text) {
            const div = document.createElement('div');
            div.textContent = text;
            return div.innerHTML;
        }

        // FeltÃ¶rlÃ©sÂƒÃ¶ltÃ¶rlÃ©sÂƒÃ©s mezÃÂ…Å‘Â€Â˜ megjelenÃÂƒÃ­tÃ¶rlÃ©sÂƒÃ©se/elrejtÃ¶rlÃ©sÂƒÃ©se
        const uploadToggle = document.getElementById('uploadToggle');
        const uploadSection = document.querySelector('.upload-section');
        
        if (uploadToggle && uploadSection) {
            uploadToggle.addEventListener('click', (e) => {
                e.preventDefault();
                uploadSection.classList.toggle('visible');
                if (uploadSection.classList.contains('visible')) {
                    uploadToggle.textContent = 'âŒÂÃ…Â’ BezÃÂƒÃ¡rÃÂƒÃ¡s / Close';
                } else {
                    uploadToggle.textContent = 'ğŸ”’Â¸Å‘Â€ÂœÃÂ¤ FeltÃ¶rlÃ©sÂƒÃ¶ltÃ¶rlÃ©sÂƒÃ©s / Upload';
                }
            });
        }
        
        // Dokumentum export funkciÃÂƒÃ³ (csak ha az elem lÃÂƒÃ©tezik)
        const exportPackage = document.getElementById('exportPackage');
        if (exportPackage) {
            exportPackage.addEventListener('click', async (e) => {
                e.preventDefault();
                if (confirm('Dokumentum export indÃÂƒÃ­tÃ¶rlÃ©sÂƒÃ¡sa? Ez ZIP fÃ¡jlÂƒÃ¡jlt kÃÂƒÃ©szÃÂƒÃ­t ÃÂƒÃ©s email-t kÃÂƒÃ¼ld.\n\nStart document export? This will create a ZIP file and send emails.')) {
                    try {
                        const originalText = exportPackage.textContent;
                        exportPackage.textContent = 'âŒÂÃ³ Export folyamatban... / Exporting...';
                        exportPackage.style.pointerEvents = 'none';
                        
                        const response = await fetch('/api/export-package', {
                            method: 'POST'
                        });
                        const result = await response.json();
                        
                        if (result.success) {
                            // Email linkek megnyitÃ¶rlÃ©sÂƒÃ¡sa natÃ¶rlÃ©sÂƒÃ­v email klienssel
                            if (result.emailLinks && result.emailLinks.length > 0) {
                                result.emailLinks.forEach((mailtoLink, index) => {
                                    setTimeout(() => {
                                        window.location.href = mailtoLink;
                                    }, index * 500); // 500ms kÃÂƒÃ©sleltetÃ¶rlÃ©sÂƒÃ©s emailenkÃÂƒÃ©nt
                                });
                            }
                            
                            alert(`âœ…Â“Å‘Â€Â¦ Export sikeres!\n\nZIP: ${result.zipFile}\nSHA-256: ${result.sha256}\nVerziÃÂƒÃ³: ${result.version}\n\nAz email kliens megnyÃÂƒÃ­lik az emailek kÃÂƒÃ¼ldÃÂƒÃ©sÃÂƒÃ©hez.\n\nExport successful!\n\nThe email client will open to send emails.`);
                        } else {
                            alert(`âŒÂÃ…Â’ Hiba: ${result.error}`);
                        }
                    } catch (error) {
                        alert(`âŒÂÃ…Â’ Export hiba: ${error.message}`);
                    } finally {
                        exportPackage.textContent = 'ğŸ”’Â¸Å‘Â€ÂœÃÂ¦ Export / Export';
                        exportPackage.style.pointerEvents = 'auto';
                    }
                }
            });
        }

        // EsemÃÂƒÃ©nykezelÃÂ…Å‘Â€Â˜k
        const searchInput = document.getElementById('searchInput');
        if (searchInput) {
            searchInput.addEventListener('input', (e) => {
                const category = document.querySelector('.filter-btn[data-category].active')?.dataset.category || 'all';
                const security = document.querySelector('.filter-btn[data-security].active')?.dataset.security || 'all';
                renderFiles(category, e.target.value, security);
            });
        }

        document.querySelectorAll('.filter-btn[data-category]').forEach(btn => {
            btn.addEventListener('click', () => {
                document.querySelectorAll('.filter-btn[data-category]').forEach(b => b.classList.remove('active'));
                btn.classList.add('active');
                const category = btn.dataset.category;
                const security = document.querySelector('.filter-btn[data-security].active')?.dataset.security || 'all';
                const searchInput = document.getElementById('searchInput');
                const searchTerm = searchInput ? searchInput.value : '';
                renderFiles(category, searchTerm, security);
            });
        });
        
        document.querySelectorAll('.filter-btn[data-security]').forEach(btn => {
            btn.addEventListener('click', () => {
                document.querySelectorAll('.filter-btn[data-security]').forEach(b => b.classList.remove('active'));
                btn.classList.add('active');
                const security = btn.dataset.security;
                const category = document.querySelector('.filter-btn[data-category].active')?.dataset.category || 'all';
                const searchInput = document.getElementById('searchInput');
                const searchTerm = searchInput ? searchInput.value : '';
                renderFiles(category, searchTerm, security);
            });
        });

        // FeltÃ¶rlÃ©sÂƒÃ¶ltÃ¶rlÃ©sÂƒÃ©si funkciÃÂƒÃ³k
        const uploadBox = document.getElementById('uploadBox');
        const fileInput = document.getElementById('fileInput');
        const uploadProgress = document.getElementById('uploadProgress');
        const progressFill = document.getElementById('progressFill');
        const uploadStatus = document.getElementById('uploadStatus');
        const uploadSuccess = document.getElementById('uploadSuccess');
        const uploadError = document.getElementById('uploadError');
        const uploadSuccessDetails = document.getElementById('uploadSuccessDetails');
        const uploadErrorDetails = document.getElementById('uploadErrorDetails');

        if (uploadBox && fileInput) {
            uploadBox.addEventListener('click', () => fileInput.click());
            
            uploadBox.addEventListener('dragover', (e) => {
                e.preventDefault();
                uploadBox.classList.add('dragover');
            });

            uploadBox.addEventListener('dragleave', () => {
                uploadBox.classList.remove('dragover');
            });

            uploadBox.addEventListener('drop', (e) => {
                e.preventDefault();
                uploadBox.classList.remove('dragover');
                const files = e.dataTransfer.files;
                if (files.length > 0) {
                    handleFileUpload(files[0]);
                }
            });
        }

        if (fileInput) {
            fileInput.addEventListener('change', (e) => {
                if (e.target.files.length > 0) {
                    handleFileUpload(e.target.files[0]);
                }
            });
        }

        async function handleFileUpload(file) {
            if (!uploadSuccess || !uploadError || !uploadProgress || !progressFill || !uploadStatus || !uploadErrorDetails) {
                console.error('Upload elements not found');
                return;
            }
            
            uploadSuccess.classList.remove('active');
            uploadError.classList.remove('active');
            uploadProgress.classList.add('active');
            progressFill.style.width = '10%';
            progressFill.textContent = '10%';
            uploadStatus.textContent = 'FÃ¡jl elÃ…Â‘kÃ©szÃ­tÃ©se... / Preparing file...';

            const formData = new FormData();
            formData.append('file', file);

            try {
                progressFill.style.width = '30%';
                progressFill.textContent = '30%';
                uploadStatus.textContent = 'OpenAI elemzÃ©s folyamatban... / OpenAI analysis in progress...';

                const response = await fetch('/api/upload', {
                    method: 'POST',
                    body: formData
                });

                progressFill.style.width = '70%';
                progressFill.textContent = '70%';
                uploadStatus.textContent = 'FÃ¡jl mentÃ©se... / Saving file...';

                if (!response.ok) {
                    const errorData = await response.json();
                    throw new Error(errorData.error || 'FeltÃ¶ltÃ©si hiba');
                }

                const result = await response.json();
                
                progressFill.style.width = '100%';
                progressFill.textContent = '100%';
                uploadStatus.textContent = 'KÃ©sz! / Done!';

                addFileToData(result.file);

                uploadSuccessDetails.innerHTML = `
                    <strong>${escapeHtml(result.file.name)}</strong><br>
                    KategÃ³ria / Category: <strong>${result.file.category}${result.file.subcategory ? '/' + result.file.subcategory : ''}</strong><br>
                    ÃÂštvonal / Path: <code>${escapeHtml(result.file.path)}</code>
                `;
                uploadSuccess.classList.add('active');
                uploadError.classList.remove('active');

                const category = document.querySelector('.filter-btn[data-category].active')?.dataset.category || 'all';
                const security = document.querySelector('.filter-btn[data-security].active')?.dataset.security || 'all';
                const searchInput = document.getElementById('searchInput');
                const searchTerm = searchInput ? searchInput.value : '';
                renderFiles(category, searchTerm, security);

                setTimeout(() => {
                    uploadProgress.classList.remove('active');
                    uploadSuccess.classList.remove('active');
                    progressFill.style.width = '0%';
                    fileInput.value = '';
                }, 3000);

            } catch (error) {
                console.error('Upload error:', error);
                uploadErrorDetails.textContent = error.message;
                uploadError.classList.add('active');
                uploadSuccess.classList.remove('active');
                uploadProgress.classList.remove('active');
            }
        }

        function addFileToData(fileInfo) {
            let targetCategory = null;
            
            if (fileInfo.category === 'docs' && fileInfo.subcategory && fileData.docs && fileData.docs[fileInfo.subcategory]) {
                targetCategory = fileData.docs[fileInfo.subcategory];
            } else if (fileData[fileInfo.category] && Array.isArray(fileData[fileInfo.category])) {
                targetCategory = fileData[fileInfo.category];
            }

            if (targetCategory && Array.isArray(targetCategory)) {
                const exists = targetCategory.some(f => f.name === fileInfo.name && f.path === fileInfo.path);
                if (!exists) {
                    targetCategory.push({
                        name: fileInfo.name,
                        path: fileInfo.path,
                        type: fileInfo.type,
                        security_level: 'medium',
                        security_label_hu: 'ğŸ”’Â¸Ã…Â¸Ã¡ KÃ¶zepesÂƒÅ‘Â€Â“ZEPES',
                        security_label_en: 'ğŸ”’Â¸Ã…Â¸Ã¡ MEDIUM',
                        security_warning_hu: '',
                        security_warning_en: '',
                        description_hu: fileInfo.description_hu,
                        description_en: fileInfo.description_en,
                        summary: fileInfo.summary,
                        content: fileInfo.content || '' // FÃ¡jlÂƒÃ¡jl tartalom keresÃÂƒÃ©shez
                    });
                }
            }
        }

        // Kezdeti renderelÃÂƒÃ©s
        renderFiles('all', '', 'all');
    </script>
</body>
</html>

